@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/XTinyTerror_#_tinyterror_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-xtinyterror.html> ;
        modgui:stylesheet <modgui/stylesheet-xtinyterror.css> ;
        modgui:screenshot <modgui/screenshot-xtinyterror.png> ;
        modgui:thumbnail <modgui/thumbnail-xtinyterror.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "xTinyTerror" ;
        modgui:model "xTerror" ;
        modgui:color "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "VOLUME" ;
            lv2:name "VOLUME" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TONE" ;
            lv2:name "TONE" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "GAIN1" ;
            lv2:name "GAIN1" ;
        ] ;
    ] .