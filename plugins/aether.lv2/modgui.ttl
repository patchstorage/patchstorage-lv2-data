@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://github.com/Dougal-s/Aether>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-aether.html> ;
        modgui:stylesheet <modgui/stylesheet-aether.css> ;
        modgui:screenshot <modgui/screenshot-aether.png> ;
        modgui:thumbnail <modgui/thumbnail-aether.png> ;
        modgui:brand "Dougal Stewart" ;
        modgui:label "Aether" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "racing" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "mix" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "dry_level" ;
            lv2:name "Dry" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "predelay" ;
            lv2:name "Predly" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "predelay_level" ;
            lv2:name "Predly Lv" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "early_level" ;
            lv2:name "Early Lv" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "late_level" ;
            lv2:name "Late Lv" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "width" ;
            lv2:name "Width" ;
        ] ;
    ] .