@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://gareus.org/oss/lv2/nodelay> ;
	lv2:port [
		lv2:symbol "delay" ;
		pset:value 0.0
	] , [
		lv2:symbol "report_latency" ;
		pset:value 1.0
	] .

