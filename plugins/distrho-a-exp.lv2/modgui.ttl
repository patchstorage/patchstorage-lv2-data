@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<urn:distrho:a-exp>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-die-expander.html> ;
        modgui:stylesheet <modgui/stylesheet-die-expander.css> ;
        modgui:screenshot <modgui/screenshot-die-expander.png> ;
        modgui:thumbnail <modgui/thumbnail-die-expander.png> ;
        modgui:brand "Distrho" ;
        modgui:label "DIE Expander" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "blue" ;
        modgui:knob "bronze" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "thr" ;
            lv2:name "Threshold" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "rat" ;
            lv2:name "Ratio" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "mak" ;
            lv2:name "Makeup" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "att" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "rel" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "sidech" ;
            lv2:name "Sidechain" ;
        ] ;
    ] .
    
