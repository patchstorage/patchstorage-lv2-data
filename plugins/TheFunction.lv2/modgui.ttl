@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://arcticanaudio.com/plugins/thefunction>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-the-function.html> ;
        modgui:stylesheet <modgui/stylesheet-the-function.css> ;
        modgui:screenshot <modgui/screenshot-the-function.png> ;
        modgui:thumbnail <modgui/thumbnail-the-function.png> ;
        modgui:brand "Arctican" ;
        modgui:label "The Function" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "white" ;
        modgui:knob "copper" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "gain" ;
            lv2:name "Gain" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "gain_l" ;
            lv2:name "Gain L" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "phase_l" ;
            lv2:name "Phase L" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "pan_l" ;
            lv2:name "Pan L" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "gain_r" ;
            lv2:name "Gain R" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "phase_r" ;
            lv2:name "Phase R" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "pan_r" ;
            lv2:name "Pan R" ;
        ] ;
    ] .