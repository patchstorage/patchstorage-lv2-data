@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/ringmod_1i1o1l>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-ringmod-with-lfo.html> ;
        modgui:stylesheet <modgui/stylesheet-ringmod-with-lfo.css> ;
        modgui:screenshot <modgui/screenshot-ringmod-with-lfo.png> ;
        modgui:thumbnail <modgui/thumbnail-ringmod-with-lfo.png> ;
        modgui:brand "Steve Harris" ;
        modgui:label "Ringmod with LFO" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "red" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "sin" ;
            lv2:name "Sine" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "tri" ;
            lv2:name "Triangle" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "saw" ;
            lv2:name "Saw" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "squ" ;
            lv2:name "Square" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "depthp" ;
            lv2:name "Depth" ;
        ] ;
    ] .
	
<http://plugin.org.uk/swh-plugins/ringmod_2i1o>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-ringmod-with-two-inputs.html> ;
        modgui:stylesheet <modgui/stylesheet-ringmod-with-two-inputs.css> ;
        modgui:screenshot <modgui/screenshot-ringmod-with-two-inputs.png> ;
        modgui:thumbnail <modgui/thumbnail-ringmod-with-two-inputs.png> ;
        modgui:brand "Steve H" ;
        modgui:label "Ringmod 2input" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "red" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] ;
    ] .