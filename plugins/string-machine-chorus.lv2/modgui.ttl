@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://jpcima.sdf1.org/lv2/string-machine-chorus>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-string-machine-chorus.html> ;
        modgui:stylesheet <modgui/stylesheet-string-machine-chorus.css> ;
        modgui:screenshot <modgui/screenshot-string-machine-chorus.png> ;
        modgui:thumbnail <modgui/thumbnail-string-machine-chorus.png> ;
        modgui:brand "Jean Pierr" ;
        modgui:label "String Machine Chorus" ;
        modgui:model "boxy" ;
        modgui:panel "1-select-4-knobs" ;
        modgui:color "purple" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "cho_model" ;
            lv2:name "Chorus model" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "cho_rate1" ;
            lv2:name "Rate 1" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "cho_depth1" ;
            lv2:name "Depht 1" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "cho_rate2" ;
            lv2:name "Rate 2" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "cho_depth2" ;
            lv2:name "Depht 2" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "cho_depth" ;
            lv2:name "Glob.Deph" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "dry_gain" ;
            lv2:name "Dry" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "wet_gain" ;
            lv2:name "Wet" ;
        ] ;
    ] .
