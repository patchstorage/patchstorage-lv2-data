## START http://github.com/blablack/deteriorate-lv2/granulator_mono

<http://github.com/blablack/deteriorate-lv2/granulator_mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-granulator-mono.html> ;
        modgui:stylesheet <modgui/stylesheet-granulator-mono.css> ;
        modgui:screenshot <modgui/screenshot-granulator-mono.png> ;
        modgui:thumbnail <modgui/thumbnail-granulator-mono.png> ;
        modgui:brand "Aurelien L" ;
        modgui:label "Granulator Mono" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "gold" ;
        modgui:knob "gold" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "inputGain" ;
            lv2:name "Input" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "grainSize" ;
            lv2:name "Gr.Size " ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "grainDensity" ;
            lv2:name "Spacing" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "grainSpread" ;
            lv2:name "Spread" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "attack" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "release" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "outputGain" ;
            lv2:name "Output" ;
        ] ;
    ] .
	
## END http://github.com/blablack/deteriorate-lv2/granulator_mono
###############################################################################

