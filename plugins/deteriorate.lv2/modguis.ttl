@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

###############################################################################
## START http://github.com/blablack/deteriorate-lv2/downsampler_mono

<http://github.com/blablack/deteriorate-lv2/downsampler_mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-downsampler-mono.html> ;
        modgui:stylesheet <modgui/stylesheet-downsampler-mono.css> ;
        modgui:screenshot <modgui/screenshot-downsampler-mono.png> ;
        modgui:thumbnail <modgui/thumbnail-downsampler-mono.png> ;
        modgui:brand "Aurelien" ;
        modgui:label "DwnSmplrMono" ;
        modgui:model "boxy" ;
        modgui:panel "1-select-1-knob" ;
        modgui:color "lava" ;
        modgui:knob "copper" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "bypass" ;
            lv2:name "Bypass" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "ratio" ;
            lv2:name "Ratio" ;
        ] ;
    ] .

## END http://github.com/blablack/deteriorate-lv2/downsampler_mono
###############################################################################

