@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://lsp-plug.in/plugins/lv2/impulse_responses_mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-lsp-ir.html> ;
        modgui:stylesheet <modgui/stylesheet-lsp-ir.css> ;
        modgui:screenshot <modgui/screenshot-mono.png> ;
        modgui:thumbnail <modgui/thumbnail-mono.png> ;
        modgui:brand "LSP" ;
        modgui:label "IR Mono" ;
        modgui:model "boxy" ;
        modgui:panel "3-knobs" ;
        modgui:color "black" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "dry" ;
            lv2:name "Dry" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "wet" ;
            lv2:name "Wet" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "g_out" ;
            lv2:name "Output" ;
        ] ;
    ] .
